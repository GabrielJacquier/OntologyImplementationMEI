PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 
PREFIX owl: <http://www.w3.org/2002/07/owl#> 
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> 
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX mei: <http://www.semanticweb.org/gabriel/ontologies/2017/8/mei#> 
SELECT ?cnae
WHERE {
	?ocupacao rdf:type mei:Ocupacao .
	?ocupacao mei:descricao ?ocupacaoInformada .
	?ocupacao mei:realiza ?atividade .
	?atividade mei:permitidaPor ?subClasse .
	?subClasse mei:codigo ?cnae .
}
GROUP BY ?cnae