PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 
PREFIX owl: <http://www.w3.org/2002/07/owl#> 
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> 
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX mei: <http://www.semanticweb.org/gabriel/ontologies/2017/8/mei#>
SELECT ?atividadeServico
WHERE {
	?servico mei:singular ?servicoSingular .
	?servico mei:plural ?servicoPlural .
	FILTER ( ?servicoPlural = ?servicoInformado || ?servicoSingular = ?servicoInformado )
	?servico mei:elementoDeAtividade ?atividadeServico .
	?requisitoServico mei:singular ?requisitoServicoSingular .
	?requisitoServico mei:plural ?requisitoServicoPlural .
	FILTER ( ?requisitoServicoSingular = ?requisitoServicoInformado || 
			 ?requisitoServicoPlural = ?requisitoServicoInformado )
	?requisitoServico mei:elementoDeAtividade ?atividadeRequisitoServico .
}